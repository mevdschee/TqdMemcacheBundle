{
    "name": "leaseweb/memcache-bundle",
    "type": "symfony-bundle",
    "description": "Memcache Doctrine caching and session storage in the Web Debug Toolbar",
    "keywords": ["memcache","memcached","cache","session"],
    "homepage": "http://www.leaseweblabs.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Maurits van der Schee",
            "email": "m.vanderschee@leaseweb.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.1",
        "ext-memcache": ">=3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-0": { "Tqd\\MemcacheBundle": "" }
    },
    "target-dir": "Tqd/MemcacheBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
